{
  "name": "jexperience-extended-tracker",
  "version": "1.0.0-SNAPSHOT",
  "description": "jExperience extended tracker",
  "scripts": {
    "build": "yarn lint:wemjs && yarn webpack:wemjs",
    "build-wemjs": "yarn lint && yarn webpack:wemjs",
    "build:analyze": "yarn build --analyze",
    "build:production": "yarn clean && yarn webpack:wemjs-prod",
    "build:production-analyze": "yarn clean && webpack -p --analyze",
    "webpack:wemjs": "node --max_old_space_size=4096 ./node_modules/webpack/bin/webpack.js --config webpack.wemjs.config.js",
    "webpack:wemjs-prod": "node --max_old_space_size=4096 ./node_modules/webpack/bin/webpack.js --config webpack.wemjs.config.js --mode=production",
    "watch": "yarn build --watch",
    "clean": "rimraf *.log test-reports ./src/main/resources/javascript/jexperience/dist/web.min.js",
    "clean:all": "yarn clean && rimraf node_modules node",
    "lint": "yarn lint:wemjs",
    "lint:wemjs": "eslint --max-warnings 1 ./src/main/resources/javascript/jexperience/live-mode/wem.js",
    "sync-pom": "sync-pom-version --use-yarn --pom-file ../../pom.xml"
  },
  "license": "SEE LICENSE IN pom.xml",
  "dependencies": {
    "apache-unomi-tracker": "^1.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/compat-data": "^7.9.0",
    "@babel/core": "^7.26.10",
    "@babel/helper-compilation-targets": "^7.27.0",
    "@babel/plugin-external-helpers": "^7.25.9",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.12.13",
    "@cyclonedx/webpack-plugin": "^3.6.1",
    "@jahia/eslint-config": "^1.1.0",
    "@jahia/stylelint-config": "^0.0.3",
    "@jahia/webpack-config": "^1.1.0",
    "babel-loader": "^8.1.0",
    "buffer": "^6.0.3",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "cheerio": "^1.0.0-rc.3",
    "clean-css": "^4.2.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^1.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.8",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-json": "^2.1.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "file-loader": "^6.2.0",
    "html-loader": "^0.5.5",
    "identity-obj-proxy": "^3.0.0",
    "imports-loader": "^0.7.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^5.0.0",
    "style-loader": "^1.2.1",
    "stylelint": "^13.2.1",
    "sync-pom-version-to-package": "^1.6.1",
    "webpack": "^5.88.2",
    "webpack-bundle-analyzer": "^4.2.0",
    "webpack-cli": "^4.2.0",
    "webpack-node-externals": "^3.0.0"
  },
  "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610"
}
